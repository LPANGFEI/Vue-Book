\n                            <span>|</span>\r\n                            {{fuzzySearch.minorCate}}\r\n                        </p>\r\n                        <p class=\"msg\">{{fuzzySearch.shortIntro}}</p>\r\n                        <p class=\"tip\">\r\n                            <span>{{fuzzySearch.latelyFollower}}</span>人气\r\n                            |\r\n                            <span>{{fuzzySearch.retentionRatio}}%</span>读者留存\r\n                        </p>\r\n                    </div>\r\n                </div>\r\n                <div class=\"blank\"></div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\nimport BScroll from \"better-scroll\";\r\nexport default {\r\n    name: \"BookSearch\",\r\n    data: () => ({\r\n        isLoading: true,\r\n        bScroll: null,\r\n        title: \"\",\r\n        fuzzySearchs: []\r\n    }),\r\n\r\n    methods: {\r\n        // 模糊搜索\r\n        async getFuzzySearch(value) {\r\n            let res = await this.$http(\"/book/fuzzy-search?query=\" + value);\r\n            res = res.data;\r\n            this.fuzzySearchs = res.books;\r\n            this.$nextTick(() => {\r\n                this.initLoading();\r\n            });\r\n        },\r\n        initLoading() {\r\n            setTimeout(() => {\r\n                this.isLoading = false;\r\n            }, 1000);\r\n        },\r\n        initBScroll() {\r\n            if (this.bScroll) {\r\n                this.bScroll.refresh();\r\n            } else {\r\n                this.bScroll = new BScroll(this.$refs.wrapper, {\r\n                    probeType: 3,\r\n                    click: true\r\n                });\r\n            }\r\n        },\r\n        goToDetail(item) {\r\n            this.$router.push({\r\n                path: \"/bookdetail\",\r\n                query: {\r\n                    title: item.title,\r\n                    id: item._id\r\n                }\r\n            });\r\n        }\r\n    },\r\n    created() {\r\n        let { title } = this.$route.query;\r\n        this.title = `搜索：${title}`;\r\n        this.getFuzzySearch(title);\r\n    },\r\n    mounted() {\r\n        this.initBScroll();\r\n    },\r\n    beforeUpdate() {\r\n        // this.bScroll.destroy();\r\n        this.bScroll = null;\r\n    }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.book-search {\r\n    width: 100%;\r\n    .wrapper {\r\n        width: 100%;\r\n        height: 100vh;\r\n        overflow: hidden;\r\n    }\r\n    .container {\r\n        width: 100%;\r\n        padding: 0 10px;\r\n        .blank {\r\n            height: 40px !important;\r\n        }\r\n    }\r\n}\r\n.text {\r\n    position: relative;\r\n    box-sizing: border-box;\r\n    padding-left: 10px;\r\n    padding-top: 5px;\r\n    line-height: 30px;\r\n    font-size: 18px;\r\n    font-weight: 500;\r\n    color: #000;\r\n    &::after {\r\n        content: \"\";\r\n        position: absolute;\r\n        left: 4px;\r\n        top: 50%;\r\n        width: 2px;\r\n        height: 20px;\r\n        background-color: #d43c33;\r\n        transform: translateY(-50%);\r\n    }\r\n}\r\n.books {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-content: center;\r\n    width: 100%;\r\n    height: 90px;\r\n    padding: 10px 0;\r\n    img {\r\n        flex: 0 0 55px;\r\n        display: block;\r\n        width: 55px;\r\n        height: 70px;\r\n    }\r\n    .desc {\r\n        flex: 1;\r\n        padding-left: 10px;\r\n        .title {\r\n            margin-bottom: 4px;\r\n            font-size: 14px;\r\n            font-weight: bold;\r\n            color: #333;\r\n        }\r\n        .tag {\r\n            font-size: 12px;\r\n            color: #999;\r\n        }\r\n        .msg {\r\n            display: -webkit-box;\r\n            -webkit-line-clamp: 2;\r\n            -webkit-box-orient: vertical;\r\n            overflow: hidden;\r\n            margin: 4px 0;\r\n            font-size: 12px;\r\n            color: #999;\r\n        }\r\n        .tip {\r\n            font-size: 12px;\r\n            color: #444;\r\n            span {\r\n                color: #b93321;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}